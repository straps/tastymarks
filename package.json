{
  "name": "tastymarks",
  "version": "1.0.0",
  "description": "Tasty Bookmarks",
  "author": "Strx <fstraps@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "dev-debug": "node --inspect node_modules/.bin/nuxt",
    "build": "export API_URL=https://tastymarks.com/ && export NODE_ENV=production && nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "rsync -a --delete --progress --exclude=node_modules --exclude=.nuxt --exclude=.git --exclude=.vscode --exclude=build --exclude=avatars --exclude=urlimg ../tastymarks strx@tastymarks.com:~",
    "buildremote": "ssh strx@tastymarks.com 'cd ~/tastymarks; npm run build'",
    "restartremote": "ssh strx@tastymarks.com 'export NODE_ENV=production; forever restart tastymarks'",
    "instdeps": "npm install && npm install --only=dev",
    "forever": "forever -w --watchDirectory build start --uid tastymarks -l /tmp/tastymarks.log --append --spinSleepTime 5000 --minUptime 5000 build/main.js"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.1",
    "@nuxtjs/axios": "^5.3.1",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.7.2",
    "cheerio": "^1.0.0-rc.2",
    "connect-pg-simple": "^4.2.1",
    "console-stamp": "^0.2.6",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.15.3",
    "express-force-domain": "0.0.6",
    "express-promise-router": "^3.0.2",
    "express-session": "^1.15.6",
    "greenlock-express": "^2.1.6",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.18.0",
    "moment": "^2.22.2",
    "nuxt": "^1.4.1",
    "pg": "^7.4.3",
    "puppeteer": "^1.5.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "source-map-support": "^0.4.15",
    "vue-chartkick": "^0.3.3",
    "vue-infinite-loading": "^2.3.1",
    "vue-swal": "^0.1.0",
    "vuetify": "^1.0.19"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0"
  }
}
